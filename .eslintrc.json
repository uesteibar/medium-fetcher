{
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "extends": [
    "standard",
    "prettier"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "comma-style": ["error", "last"],
    "no-extra-semi": "error",
    "object-curly-spacing": ["error", "always", { "arraysInObjects": true, "objectsInObjects": true }],
    "semi": ["error", "never"]
  }
}
